{
  "name": "microservice",
  "version": "0.0.1",
  "description": "REST API build with Node.js",
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv/config -r esm app.js",
    "debug": "node --inspect -r dotenv/config -r esm app.js",
    "prettier": "prettier --write \"**/*.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint .",
      "git add"
    ]
  },
  "author": {
    "name": "Sergey Onufrienko <sergey@onufrienko.com>",
    "url": "https://github.com/sonufrienko"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonufrienko/microservice"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "esm": "^3.0.84",
    "fastify": "^1.13.3",
    "mongodb": "^3.1.10",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
